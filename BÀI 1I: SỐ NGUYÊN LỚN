import re
from math import *
from itertools import *
from collections import *
from functools import *
import sys
from typing import *


def is_32_bit_integer(num):
    return num <= 2 ** 31 - 1


def is_64_bit_integer(num):
    return num <= 2 ** 63 - 1


def main():
    s = 0
    while True:
        try:
            line = input()
            numbers = re.findall(r'\d+', line)
            for num in numbers:
                number = int(num)
                if not is_32_bit_integer(number) and is_64_bit_integer(number):
                    s += number
        except EOFError:
            break
    print(s)


if __name__ == '__main__':
    main()
